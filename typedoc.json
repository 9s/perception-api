{
  "entryPoints": [
    "./cs2.d.ts",
    "./rivals.d.ts",
    "./universal.d.ts",
    "./valorant.d.ts"
  ],
  "out": "docs",
  "includeVersion": true,
  "tsconfig": "tsconfig.json",
  "name": "perception.cx api",
  "plugin": [
    "@typhonjs-typedoc/typedoc-theme-dmt",
    "typedoc-plugin-extras"
  ],
  "theme": "default-modern",
  "customTitle": "Perception TypeScript API",
  "dmtFavicon": "https://perception.cx/data/assets/logo/perception_logo_1.png",
  "dmtLinksService": {
    "Discord": "@qqwwwwwxx",
    "GitHub": "https://github.com/9s/perception-api",
    "NPM": "https://www.npmjs.com/package/perception-api"
  },
  "dmtNavigation": {
    "moduleIcon": false,
    "style": "full"
  },
  "dmtSearch": {
    "fullName": false,
    "limit": 10
  },
  "dmtSettings": {
    "animation": true
  }
}
